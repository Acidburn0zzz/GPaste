_gpaste()
{
    local cur opts
    COMPREPLY=()
    cur="${COMP_WORDS[${COMP_CWORD}]}"
    opts="history raw-history add get set delete file empty start daemon daemon-reexec stop quit preferences settings version -v --version help -h --help"
    COMPREPLY=( $(compgen -W "$opts" -- $cur ) )
}

complete -F _gpaste gpaste
