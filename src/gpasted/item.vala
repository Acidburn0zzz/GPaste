namespace GPaste {

    namespace Daemon {

        public enum ItemKind {
            STRING,
            IMAGE
        }

        public struct Item {
            public ItemKind kind;

            // This means either the String, or the path to the file.
            public string val;

            public Item (ItemKind kind, string val) {
                this.kind = kind;
                this.val = val;
            }
        }

    }

}

